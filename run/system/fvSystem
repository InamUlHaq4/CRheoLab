FoamFile
{
    version     2.0;
    format      ascii;
    class       fvSystem;
    location    "system";
    object      fvSystem;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

absNormResidual 1E-6;
relNormResidual 1E-3;
wSOR 1.2;
solverModel Jacobi; // Jacobi, GaussSiedel, SOR
matrixFormat CSR; //lOLists // CSR


T1
{
absNormResidual 1E-6;
relNormResidual 1E-3;
wSOR 1.2;
solverModel Jacobi; // Jacobi, GaussSiedel, SOR
matrixFormat CSR; //lOLists // CSR
}

T2
{
absNormResidual 1E-6;
relNormResidual 1E-3;
wSOR 1.2;
solverModel Jacobi; // Jacobi, GaussSiedel, SOR
matrixFormat CSR; //lOLists // CSR
}


